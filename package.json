{
	"name": "pictures-converter",
	"version": "1.0.0",
	"description": "Node.js image converter (JPEG, PNG, WebP, AVIF, TIFF, GIF)",
	"bin": {
		"pictures-converter": "./bin/pictures-converter.js"
	},
	"directories": {
		"test": "test"
	},
	"type": "commonjs",
	"scripts": {
		"test": "npx nyc --reporter=lcov mocha 'test/**/*.js'",
		"test-local": "npx nyc --reporter=text mocha 'test/**/*.js'",
		"lint": "npx eslint . --fix"
	},
	"repository": {
		"url": "https://github.com/montoyadamien/pictures-converter.git",
		"type": "git"
	},
	"bugs": {
		"url": "https://github.com/montoyadamien/pictures-converter/issues"
	},
	"author": "Damien Montoya <damien.montoya26@gmail.com>",
	"license": "Apache-2.0",
	"keywords": [
		"jpeg",
		"png",
		"webp",
		"avif",
		"tiff",
		"gif",
		"svg",
		"jp2",
		"dzi",
		"image",
		"converter"
	],
	"dependencies": {
		"commander": "^12.1.0",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-mocha": "^10.5.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.14.0",
		"mocha": "^11.0.1",
		"nyc": "^17.1.0",
		"prettier": "^3.4.2"
	}
}
